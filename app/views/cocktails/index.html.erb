<h1>Cocktails</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
      <% @cocktails.each do |cocktail| %>
      <tr>
        <td><%= cocktail.name %></td>
        <td><%= link_to "Show", cocktail_path(cocktail)%> </td>
      </tr>
      <% end %>
  </tbody>
</table>
<%= link_to "Add a new cocktail", new_cocktail_path%>
